    <!--[if lt IE 11]><![endif]-->

      <style type="text/css">
          .ie-not{
             display: none;
          }
          .ie-not-content{
            overflow: auto;
            position: absolute;
            bottom:0;
            left:0;
            right:0;
            top:0;
            font-size: 16px;
            padding: 50px;
            text-align: left;
          }
          .ie-not-header{
            font-weight: 800;
            padding-bottom: 10px;
          }

          .ie-not-content h5 {
            font-weight: 800;
            margin-top: 30px
          }

          .ie-not-display{
          display:block;
          position: fixed;
          top:0%;
          width: 100%;
          height: 100%;
          z-index: 999999 !important;
          text-align: center;
          font-size: 3em;
          padding: 5px;
          color: red;
          background-color: white;
          color: black;
          }
          .display-content:{
            visibility: visible;
          }
          .hide-content:{
            visibility: hidden;
          }
      </style>
      </head>
<body>
    <div class="ie-not" id="ie-not">
        <div class="ie-not-content">
          <div class="ie-not-header">
            <p>Sorry, the browser you are currently using is not supported by example.com
              Please consider updating your browser or using one of the following: </p>
            <p> Same Message in Other Languages</p>
        </div>
      </div>
  </div>

  <!--[if gte IE 10]> -->
    <div id="root" class = "hide-content"><%- reactOutput %></div>
    <div id="fb-root"></div>
    <script>
    //<!-- IE Support Check -->
    function getIEVersion(){
        var agent = navigator.userAgent;
        var reg = /MSIE\s?(\d+)(?:\.(\d+))?/i;
        var matches = agent.match(reg);
        if (matches != null) {
            return { major: matches[1], minor: matches[2] };
        }
        return { major: "-1", minor: "-1" };
    }

    var ie_version =  getIEVersion();
    if(ie_version.major < 11 && ie_version.major > 4){
      var d = document.getElementById("ie-not");
      d.className = "ie-not-display";
    }else{
      var d = document.getElementById("root");
      d.className = "display-content";
    }

    </script>
    <script id="preloadedData" type="application/json">
        <%- JSON.stringify(preloadedData).replace(/\//g, '\\/') %>
    </script>

    <script src="/assets/js/jQuery.js"></script>
    <script src="/assets/js/bootstrap.min.js"></script>
    <script src="/assets/js/custom.js"></script>
    <script src="/app.js"></script>
    <!-- <![endif]-->

</body>
